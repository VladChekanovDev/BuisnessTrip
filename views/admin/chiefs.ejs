<%- include('../includes/head.ejs') %>
    <a href="/admin/<%= userId %>/chiefs/add-chief">Добавить главу отдела</a><br>
    <% for (let chief of chiefs) {%>
        <%= chief.lastName %> <br>
        <%= chief.firstName %> <br>
        <%= chief.patronymic ? chief.patronymic : '' %><br>
        <%= chief.department.name %>
        <form action="/admin/chiefs/delete-chief" method="post">
            <input type="hidden" name="chiefId" value="<%= chief.id %>">
            <input type="hidden" name="userId" value="<%= userId %>">
            <button type="submit">Удалить</button>
        </form>
        <a href="/admin/<%= userId%>/chiefs/edit-chief/<%= chief.id %>">Редактировать</a>
        <hr>
    <% }; %>
    <a href="/admin/<%= userId %>">На панель</a>
<%- include('../includes/end.ejs') %>